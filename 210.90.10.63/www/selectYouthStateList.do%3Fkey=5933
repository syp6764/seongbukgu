//인기 검색어를 가져와서 동적으로 추가하는 함수
function loadPopularKeywords(type) {
    const url = '/search/api/ppk.jsp?type=' + type;
    const popularList = document.getElementById("popularList");

    // AJAX 요청
    fetch(url)
        .then(response => response.json())
        .then(data => {
            // 기존 목록 초기화
            popularList.innerHTML = "";

            // 데이터 순회하며 li 요소 생성 및 추가
            data.forEach(item => {
                const kwd = item.kwd; // 검색어
                const encodedKwd = encodeURIComponent(kwd); // URL 인코딩
                const listItem = `
                    <li class="keyword_item">
                        <a href="/search/search.jsp?kwd=`+encodedKwd+`" class="keyword_anchor">
                            <span>`+kwd+`</span>
                        </a>
                    </li>
                `;
                popularList.insertAdjacentHTML("beforeend", listItem);
            });
        })
        .catch(error => {
            console.error("인기 검색어를 가져오는 중 오류 발생:", error);
        });
}

// 페이지 로드 시 실행 (기본 daily로 호출)
document.addEventListener("DOMContentLoaded", function () {
    //loadPopularKeywords("daily");

     loadPopularKeywords("weekly");
});


document.getElementById('sel3').addEventListener('change', function () {
    const today = new Date();
    const startInput = document.getElementById('cal_start');
    const endInput = document.getElementById('cal_end');
    let startDate = '';
    let endDate = '';

    switch (this.value) {
        case 'opt1': // 1주
            startDate = new Date(today);
            startDate.setDate(today.getDate() - 7);
            endDate = today;
            break;
        case 'opt2': // 1개월
            startDate = new Date(today);
            startDate.setMonth(today.getMonth() - 1);
            endDate = today;
            break;
        case 'opt3': // 1년
            startDate = new Date(today);
            startDate.setFullYear(today.getFullYear() - 1);
            endDate = today;
            break;
        default: // 전체
            startDate = '';
            endDate = '';
    }

    // 날짜 형식 지정
    function formatDate(date) {
        if (!date) return '';
        const yyyy = date.getFullYear();
        let mm = date.getMonth() + 1; // 월은 0부터 시작하므로 +1
        let dd = date.getDate();

        // 두 자릿수로 포맷 (예: 01, 02 등)
        if (mm < 10) mm = '0' + mm;
        if (dd < 10) dd = '0' + dd;

        return yyyy + '-' + mm + '-' + dd;
    }

    startInput.value = formatDate(startDate);
    endInput.value = formatDate(endDate);
});


</script>
</body>
</html>
